{
  "name": "waxin-agent",
  "version": "0.1.0",
  "description": "TUI for Bot Manager Agent debugging and testing",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun run scripts/dev.ts",
    "dev:direct": "bun run --watch src/index.ts",
    "logs": "bun run scripts/log-viewer.ts",
