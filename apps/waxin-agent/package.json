  },
  "dependencies": {
    "@mks2508/better-logger": "^4.0.0",
    "@mks2508/bot-manager-agent": "workspace:*",
    "@mks2508/no-throw": "^0.1.0",
    "@mks2508/opentui-image": "^0.2.0",
    "@opentui/core": "^0.1.69",
    "@opentui/react": "^0.1.69",
    "chalk": "^5.6.2",
    "dotenv": "^17.2.3",
    "figures": "^6.1.0",
    "react": "^19.0.0",
    "react-reconciler": "^0.32.0",
    "terminal-link": "^5.0.0"
